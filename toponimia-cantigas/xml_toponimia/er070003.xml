      </EDIANT>

      <EDIANT T="PALEO">
        <P>Machado 731</P>
      </EDIANT>

      <EDIANT T="PALEO">
        <P>Braga 370</P>
      </EDIANT>

      <EDIANT T="DIVUL">
        <P>Alvar/Beltrán, Antología, 108</P>
      </EDIANT>

      <EDIANT T="DIVUL">
        <P>Torres, Poesia trovadoresca, p. 309</P>
      </EDIANT>

      <EDIANT T="DIVUL">
