        <ESQRIM TIPO="aaB"></ESQRIM>

        <INFMET>
          <ESQMET>10 10 4</ESQMET>
        </INFMET>

        <NUMESQ>26:87</NUMESQ>
      </CABGRU>

      <BODY>
        <L AC="+--+---+-+" ERI="assy" N="1" NS="10" RI="y">
          <LT>Diss' a fremo<SEG C="M" ID="1" N="SIN">sa en</SEG> Bonaval
          assy:</LT>

          <LL>Diss' a fremosa en Bonaval assy:</LL>

          <TOPO CAT="LCULTO">Bonaval</TOPO>
        </L>

        <L AC="-+-+--+--+" ERI="aqui" N="2" NS="10" RI="i">
          <LT>"Ay, Deus! Hu <SEG C="R" ID="2" N="CF">é</SEG> meu amigo d'
          aqui,</LT>

          <LL>"Ay, Deus! Hu é meu amigo d' aqui,</LL>
        </L>

        <REFRAN>
          <L AC="---+" ERI="Bonaval" N="3" NS="4" RI="al">
            <LT>de Bonaval?</LT>

            <LL>de Bonaval?</LL>

            <TOPO CAT="LCULTO">Bonaval</TOPO>
          </L>
        </REFRAN>
      </BODY>
    </GRUPO>

    <GRUPO NUM="2" TIPO="ESTROFA">
      <CABGRU NUTOTVER="2+1">
        <ESQRIM TIPO="aaB"></ESQRIM>

        <INFMET>
