          <LL>porquê non foy migo na sagraçon</LL>
        </L>

        <REFRAN>
          <L AC="---+" ERI="Bonaval" N="3" NS="4" RI="al">
            <LT>de Bonaval.</LT>

            <LL>de Bonaval.</LL>

            <TOPO CAT="LCULTO">Bonaval</TOPO>
          </L>
        </REFRAN>
      </BODY>
    </GRUPO>

    <GRUPO NUM="3" TIPO="ESTROFA">
