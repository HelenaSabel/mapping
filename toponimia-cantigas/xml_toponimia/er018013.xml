        <NUMESQ>163:17</NUMESQ>
      </CABGRU>

      <BODY>
        <L AC="---+---+-+" ERI="sen" N="1" NS="10" RI="en">
          <LT>En esto fez come de bõo sen:</LT>

          <LL>En esto fez come de bõo sen:</LL>
        </L>

        <L AC="-----+---+-" ERI="conhocia" N="2" NS="10" RI="ia">
          <LT>en filhar adail que conhocia;</LT>

          <LL>en filhar adail que conhocia;</LL>
        </L>

        <L AC="---+---+-+-" ERI="sabia" N="3" NS="10" RI="ia">
          <LT>que estes passos maos ben <SEG C="R" ID="6" N="RD">sabia</SEG>,</LT>

          <LL>que estes passos maos ben sabia,</LL>
        </L>

        <L AC="---+--+--+" ERI="ben" N="4" NS="10" RI="en">
          <LT>e el guardo-o logu&#39; enton mui ben</LT>

          <LL>e el guardo-o logu&#39; enton mui ben</LL>
        </L>

        <L AC="---+--+--+" ERI="leixar" N="5" NS="10" RI="ar">
          <LT><SEG C="R" ID="2" N="CP">deles e fez-lhi de destro <SEG C="R"
ID="3" N="PR">leixar</SEG></SEG></LT>

          <LL>deles e fez-lhi de destro leixar</LL>
        </L>

        <L AC="--+---+--+" ERI="lidar" N="6" NS="10" RI="ar">
          <LT>lealdad&#39; e de seestro lidar</LT>

          <LL>lealdad&#39; e de seestro lidar</LL>
        </L>

        <L AC="---+---+-+-" ERI="-ia" N="7" NS="10" RI="ia">
          <LT>...<SEG C="I" ID="7" N="RE">-ia</SEG>.</LT>

          <LL>...-ia.</LL>
        </L>
      </BODY>
    </GRUPO>

    <GRUPO NUM="3" TIPO="ESTROFA">
      <CABGRU NUTOTVER="7">
        <ESQRIM TIPO="abbaccb"></ESQRIM>

        <INFMET>
          <ESQMET>10 10&#39; 10&#39; 10 10 10 10&#39;</ESQMET>

          <NOTMET>
            <P>O verso 7 é hipérmetro.</P>
          </NOTMET>
        </INFMET>

        <NUMESQ>163:17</NUMESQ>
      </CABGRU>

      <BODY>
        <L AC="---+--+--+" ERI="sabedor" N="1" NS="10" RI="or">
          <LT>O adail é mui <SEG C="I" ID="8" N="RE">gran</SEG> <SEG C="R"
          ID="6" N="RD">sabedor</SEG>,</LT>

          <LL>O adail é mui gran sabedor,</LL>
        </L>

        <L AC="---+--+--+-" ERI="carreira" N="2" NS="10" RI="eira">
          <LT>que o guiou per aquela carreira:</LT>

          <LL>que o guiou per aquela carreira:</LL>
        </L>

        <L AC="---+--+--+-" ERI="fronteira" N="3" NS="10" RI="eira">
          <LT>por que <SEG C="I" ID="9" N="RE">o</SEG> fez desguiar da
          fronteira</LT>

          <LL>por que o fez desguiar da fronteira</LL>
        </L>

        <L AC="---+--+--+" ERI="senhor" N="4" NS="10" RI="or">
          <LT>e en tal guerra leixar seu senhor;</LT>

          <LL>e en tal guerra leixar seu senhor;</LL>
        </L>

        <L AC="--+----+-+" ERI="leixar" N="5" NS="10" RI="ar">
          <LT><SEG C="R" ID="2" N="CP">e direi-vos al que lhi fez <SEG C="R"
ID="3" N="PR"><SEG C="R" ID="4" N="PV">leixar</SEG></SEG></SEG>:</LT>

          <LL>e direi-vos al que lhi fez leixar:</LL>
        </L>

        <L AC="---+--+--+" ERI="ficar" N="6" NS="10" RI="ar">
          <LT>ben que podera fazer por ficar,</LT>

          <LL>ben que podera fazer por ficar,</LL>
        </L>

        <L AC="----+-----+-" ERI="Talaveira" N="7" NS="11" RI="eira">
          <LT>e fe<SEG C="M" ID="10" N="SIN">ze-o</SEG> poer aalen a
          Talaveira.</LT>

          <LL>e feze-o poer aalen a Talaveira.</LL>

          <TOPO CAT="CIUDAD">Talaveira</TOPO>
