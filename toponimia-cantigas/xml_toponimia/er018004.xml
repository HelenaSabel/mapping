<RM BCA = "V" NM = "76" F = "6r" MU = "NON" EM = "NO" I.FI = "lv006r"></RM>
<RM BCA = "V" NM = "76" F = "6v" MU = "NON" EM = "NO" I.FI = "lv006v"></RM>
