      <BODY>
        <L AC="---+---+" ERI="conquereu" N="1" NS="8" RI="eu">
          <LT>O que <SEG C="R" ID="1" N="AE">Valença</SEG> <SEG C="R" ID="2"
          N="RD">conquereu</SEG></LT>

          <LL>O que Valença conquereu</LL>

          <TOPO CAT="CIUDAD">Valença</TOPO>
        </L>

        <L AC="---+---+" ERI="aver" N="2" NS="8" RI="er">
          <LT>por sempre mays <SEG C="R" ID="1" N="AE">valenç&#39;</SEG> aver,</LT>

          <LL>por sempre mays valenç&#39; aver,</LL>
        </L>

        <L AC="-+-----+" ERI="manteer" N="3" NS="8" RI="er">
          <LT><SEG C="R" ID="1" N="AE">Valença</SEG> se quer <SEG C="R" ID="3"
          N="RD">manteer</SEG>,</LT>

          <LL>Valença se quer manteer,</LL>

          <TOPO CAT="CIUDAD">Valença</TOPO>
        </L>

        <L AC="----+--+" ERI="entendeu" N="4" NS="8" RI="eu">
          <LT>e ssenpr&#39; en <SEG C="R" ID="1" N="AE">valen<SEG C="R" ID="4"
N="SIN">ça</SEG></SEG> <SEG C="R" ID="4" N="SIN">en</SEG>tendeu.</LT>

          <LL>e ssenpr&#39; en valença entendeu.</LL>
        </L>

        <L AC="---+---+" ERI="senhor" N="5" NS="8" RI="or">
          <LT>E de <SEG C="R" ID="1" N="AE">Valença</SEG> é senhor,</LT>

          <LL>E de Valença é senhor,</LL>

          <TOPO CAT="CIUDAD">Valença</TOPO>
        </L>

        <L AC="--+" ERI="loor" N="6" NS="8" RI="or">
          <LT>poys el manten prez e loor</LT>

          <LL>poys el manten prez e loor</LL>
        </L>

        <L AC="---+---+" ERI="valer" N="7" NS="8" RI="er">
          <LT>e pres <SEG C="R" ID="5" N="CF"><SEG C="R" ID="1" N="AE">Valença</SEG></SEG>
          por valer.</LT>

          <LL>e pres Valença por valer.</LL>

          <TOPO CAT="CIUDAD">Valença</TOPO>
        </L>
      </BODY>
    </GRUPO>

