        <L AC="----+--+" ERI="pram" N="2" NS="8" RI="an">
          <LT>por aver <SEG C="R" ID="1" N="AE">Valença</SEG>, <SEG C="R"
          ID="8" N="PV">de pram</SEG>;</LT>

