        <L AC="---+---+" ERI="diran" N="3" NS="8" RI="an">
          <LT>e por <SEG C="R" ID="1" N="AE">valença</SEG> <SEG C="R" ID="9"
          N="PV">lhi diran</SEG></LT>

          <LL>e por valença lhi diran</LL>
        </L>

        <L AC="---+---+" ERI="gaanhou" N="4" NS="8" RI="ou">
          <LT>que ben <SEG C="R" ID="1" N="AE">Valença</SEG> gaanhou.</LT>

          <LL>que ben Valença gaanhou.</LL>

          <TOPO CAT="CIUDAD">Valença</TOPO>
